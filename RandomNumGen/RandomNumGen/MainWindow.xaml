<Window x:Class="RandomNumGen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RandomNumGen"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="JamJam Random Number Generator Grade For Test" HorizontalAlignment="Center" Margin="0,39,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label Content="Min" HorizontalAlignment="Left" Margin="153,114,0,0" VerticalAlignment="Top" FontSize="18"/>
        <Label Content="Max" HorizontalAlignment="Left" Margin="146,0,0,0" VerticalAlignment="Center" FontSize="18"/>
        <TextBox x:Name="txtBoxMin" HorizontalAlignment="Left" Margin="113,153,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="16"/>
        <TextBox x:Name="txtBoxMax" HorizontalAlignment="Left" Margin="108,239,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="14"/>
        <Button Content="RUN" HorizontalAlignment="Left" Margin="146,306,0,0" VerticalAlignment="Top" FontSize="20" Click="Button_Click" IsDefault="True"/>
        <TextBlock x:Name="txtBlock" HorizontalAlignment="Left" Margin="462,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="48"/>


    </Grid>
</Window>
